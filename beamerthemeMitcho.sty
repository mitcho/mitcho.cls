\mode<presentation>

\useinnertheme[shadow=true]{rounded}
\useoutertheme{default}
\usecolortheme{orchid}
\usecolortheme{whale}

\setbeamercolor{item projected}{bg=mitred}
\setbeamercolor{subitem projected}{bg=mitred}
\setbeamercolor{subsubitem projected}{bg=mitred}
\setbeamercolor{itemize item}{fg=mitred}
\setbeamercolor{itemize subitem}{fg=mitred}
\setbeamercolor{itemize subsubitem}{fg=mitred}
\setbeamercolor{palette primary}{bg=mitred}
\setbeamertemplate{items}[circle]

\setbeamertemplate{navigation symbols}{}%remove navigation symbols

\setbeamerfont{block title}{size={}}

\mode
<all>
